	gcc -o arraytest unit_tests/arraytest.c ../src/ADT/mesinkata.c ../src/ADT/mesinkarakter.c ../src/ADT/array.c ../src/functions.c -lm

matriks :
	gcc -o matrikstest unit_tests/matrikstest.c ../src/ADT/mesinkata.c ../src/ADT/mesinkarakter.c ../src/ADT/matriks.c ../src/functions.c -lm

queue :
	gcc -o queuetest unit_tests/queuetest.c ../src/ADT/mesinkata.c ../src/ADT/mesinkarakter.c ../src/ADT/queue.c ../src/functions.c -lm

